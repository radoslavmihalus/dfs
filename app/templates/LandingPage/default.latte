                                            }, 800, 'easeInOutCirc');
                                            $("#joinmeli").removeClass("active");
                                            $("#whoweareli").removeClass("active");
                                            $("#howwegrowli").addClass("active");
                                            $("#whatweofferli").removeClass("active");
                                            $("#contactusli").removeClass("active");
