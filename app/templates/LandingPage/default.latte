                                    $('html,body').animate({
                                        scrollTop: $(target).offset().top - 89
                                    }, 800, 'easeInOutCirc');

                                    $("#joinmeli").removeClass("active");
                                    $("#whoweareli").addClass("active");
                                    $("#howwegrowli").removeClass("active");
                                    $("#whatweofferli").removeClass("active");
                                    $("#contactusli").removeClass("active");
                                });

                                $('#howwegrow').click(function (event) {
                                    event.preventDefault();
                                    var target = "#follow";

                                    $('html,body').animate({
                                        scrollTop: $(target).offset().top - 69
                                    }, 800, 'easeInOutCirc');
                                    $("#joinmeli").removeClass("active");
                                    $("#whoweareli").removeClass("active");
                                    $("#howwegrowli").addClass("active");
                                    $("#whatweofferli").removeClass("active");
                                    $("#contactusli").removeClass("active");
