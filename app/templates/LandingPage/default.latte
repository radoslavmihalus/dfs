        <script>
            $(function () {
                $('[data-toggle="tooltip"]').tooltip()
            });

            $("#logo").click(function (event)
            {
                event.preventDefault();
                var target = "#join";

                $('html,body').animate({
                    scrollTop: $(target).offset().top - 69
                }, 800, 'easeInOutCirc');

                $("#joinmeli").removeClass("active");
                $("#whoweareli").removeClass("active");
                $("#howwegrowli").removeClass("active");
                $("#whatweofferli").removeClass("active");
                $("#contactusli").removeClass("active");
            });

            $("#joinme").click(function () {
                $("#login_link").removeClass("active");
                $("#register_link").addClass("active");
                $("#login").removeClass("in");
                $("#login").removeClass("active");
                $("#registration").addClass("in");
                $("#registration").addClass("active");

                event.preventDefault();
                var target = "#join";

                $('html,body').animate({
                    scrollTop: $(target).offset().top - 69
                }, 800, 'easeInOutCirc');

                $("#joinmeli").addClass("active");
                $("#whoweareli").removeClass("active");
                $("#howwegrowli").removeClass("active");
                $("#whatweofferli").removeClass("active");
                $("#contactusli").removeClass("active");
            });

            $('#whoweare').click(function (event) {
                event.preventDefault();
                var target = "#discover";

                $('html,body').animate({
                    scrollTop: $(target).offset().top - 89
                }, 800, 'easeInOutCirc');

                $("#joinmeli").removeClass("active");
                $("#whoweareli").addClass("active");
                $("#howwegrowli").removeClass("active");
                $("#whatweofferli").removeClass("active");
                $("#contactusli").removeClass("active");
            });

            $('#howwegrow').click(function (event) {
                event.preventDefault();
                var target = "#follow";

                $('html,body').animate({
                    scrollTop: $(target).offset().top - 69
                }, 800, 'easeInOutCirc');
                $("#joinmeli").removeClass("active");
                $("#whoweareli").removeClass("active");
                $("#howwegrowli").addClass("active");
                $("#whatweofferli").removeClass("active");
                $("#contactusli").removeClass("active");
            });

            $('#whatweoffer').click(function (event) {
                event.preventDefault();
                var target = "#about";

                $('html,body').animate({
                    scrollTop: $(target).offset().top - 89
                }, 800, 'easeInOutCirc');

                $("#joinmeli").removeClass("active");
                $("#whoweareli").removeClass("active");
                $("#howwegrowli").removeClass("active");
                $("#whatweofferli").addClass("active");
                $("#contactusli").removeClass("active");
            });

            $('#contactus').click(function (event) {
                event.preventDefault();
                var target = "#contact";

                $('html,body').animate({
                    scrollTop: $(target).offset().top - 69
                }, 800, 'easeInOutCirc');

                $("#joinmeli").removeClass("active");
                $("#whoweareli").removeClass("active");
                $("#howwegrowli").removeClass("active");
                $("#whatweofferli").removeClass("active");
                $("#contactusli").addClass("active");
            });
        </script>
        {foreach $flashes as $flash}
            <script type="text/javascript">
                ShowMessageModal(enumNotificationType.information, {$flash->message});
