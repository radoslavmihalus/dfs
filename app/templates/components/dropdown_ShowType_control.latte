{define #dropdown_ShowType_control}
<div class="form-group">
    <span style="font-size: 13px;display:block;margin-bottom: 5px;font-weight:bold;">{_$description} <i data-toggle="popover" data-trigger="hover" title="{_$description}" data-content="{_$tooltip}" class="fa fa-question-circle tooltip_brown fa-1x" style="color:#83765E;cursor:pointer;"></i></span>
    <select class="form-control font_size_13px" n:name="{$name}" id="{$id}">
        <option value="" selected disabled>{_ 'Please select'}</option>
        <option value="Regional">{_'Regional'}</option>
        <option value="Club">{_'Club'}</option>
        <option value="SpecialCAC">{_'Special CAC'}</option>
        <option value="NationalCAC">{_'National CAC'}</option>
        <option value="NationalCAC, NW">{_'National CAC, NW'}</option>
        <option value="InternationalCACIB">{_'International CACIB'}</option>
        <option value="EuropeanShow">{_'European show'}</option>
        <option value="WorldShow">{_'World show'}</option>
    </select>
</div>
{/define}