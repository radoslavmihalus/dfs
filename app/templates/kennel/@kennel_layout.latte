        <link rel="stylesheet" href="www/css/selectivity-full.css">
        <script type="text/javascript" src="www/js/selectivity-full.js"></script>
        <style>
            #ajax-spinner {
                position:fixed;
                left: 50%;
                top: 50%;
                margin: -15px 0 0 -15px;
                background: white url('www/img/ajax-loader.gif') no-repeat 50% 50%;
                padding: 13px;
                border: 2px solid #CCC;
                font-size: 0;
            }
            /*            .loading_bar_body {
                            height: 100%;
                            width: 100%;
                            top:0;
                            left:0;
                            background: url(img/ajax-loader.gif) no-repeat 50% 50%;
                        }*/
        </style>
        <!-- noty - popup alerts -->
        <script src="www/js/jquery-noty/packaged/jquery.noty.packaged.min.js" type="text/javascript"></script>
        <script src="www/js/jquery-noty/themes/bootstrap.js" type="text/javascript"></script>
        <!-- Loading animation -->
        <script src="www/js/jquery.nimble.loader.js" type="text/javascript"></script>
        <script src="www/templates/js/pageinit.js" type="text/javascript"></script>
        <script type="text/javascript">
            $(document).ready(function ()
            {
                var wheight = $(window).height() - 51;
                $(".user-menu").css("max-height", wheight);
            });
            $(window).resize(function () {
                var wheight = $(window).height() - 51;
                $(".user-menu").css("max-height", wheight);
            });
        </script>


    </head>
    <body>
        <!-- Modal Flash control  -->
        {include '../components/modal_control.latte'}
        <!-- top-menu -->
        {include '../menu/top-menu.latte'}
        <!-- /top-menu -->
        <!-- user-menu -->
        {include '../menu/user-menu.latte'}
        <!--/user-menu -->

        <!-- Main content -->
        <div class="container-fluid content-wrapper">
            <div class="row" style="margin-left: 0px;margin-right: 0px;">
                <div class="col-md-12" style="padding-left: 5px;padding-right: 5px;">
                    {include 'kennel_header.latte'}
                    <div class="col-md-12">
                        {include content}
                    </div>
                </div>
            </div>
        </div>
        <!-- /Main content -->
        <!-- Scripts -->

        <script type="text/javascript">
            $(function () {
                // vhodně nastylovaný div vložím po načtení stránky
                $('<div id="ajax-spinner"></div>').appendTo("body").ajaxStop(function () {
                    // a při události ajaxStop spinner schovám a nastavím mu původní pozici
                    $(this).hide().css({
                        position: "fixed",
                        left: "50%",
                        top: "50%"
                    });
                }).hide();
            });

// zajaxovatění odkazů provedu takto
            $("a.ajax").live("click", function (event) {
                event.preventDefault();

                $.get(this.href);

                // zobrazení spinneru a nastavení jeho pozice
                $("#ajax-spinner").show().css({
                    position: "absolute",
                    left: event.pageX + 20,
                    top: event.pageY + 40
                });
            });
        </script>
        {foreach $flashes as $flash}
            <script type="text/javascript">
                if({$flash->type} == "error")
                {
                    
                }
                ShowMessageModal(enumNotificationType.information, {$flash->message});
            </script>
        {/foreach}
    </body>
</html>