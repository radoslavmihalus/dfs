{block content}
<div class="panel-default col-lg-12 col-md-12 col-xs-12" style="font-size: 13px;min-width: 244px">
    <h1><i class="glyphicons glyphicons-user"></i>&nbsp;&nbsp;{_ 'Owners of purebred dogs'}&nbsp;<button class="btn btn-default" style="text-transform: uppercase;" role="button" data-toggle="collapse" href="#OwnerFilter" aria-expanded="false" aria-controls="collapseExample"><i class="fa fa-search"></i></button></h1>
    <div class="collapse" id="OwnerFilter">
        {include 'owner_list_filter.latte'}
    </div>
    {control vp, right, large}
    <div class="row">
        <!-- List profile component -->
        {includeblock 'owner_list_item.latte'}
        {foreach $owners as $owner}
            <div class="col-lg-4 col-md-6 col-xs-12 padding">
                {include owner_list_item owner=>$owner}
            </div>
        {/foreach}
        <!-- / List profile component -->
    </div>
    {control vp, right, large}
</div>  
{/block}
