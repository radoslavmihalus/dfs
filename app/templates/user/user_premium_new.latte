<div id="premium-benefits" class="row benefits-placeholder">
    <h3 class="body-caption">Grow up your audience</h3>
    <div class="col-md-10 col-md-offset-1">
            <p style="font-family: 'Roboto Condensed', sans-serif;font-size: 20px !important;text-align: center;margin-bottom: 15px;" class="premium-reason-paragraph">More than 600.000 pageviews and 10.000 visitors each day from more than 200 countries will provide you potentially interested people from all over the world and the right place for your presentation.</p>
        </div>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
        <script type="text/javascript">
            google.charts.load('current', {
                'packages': ['geochart'],
                // Note: you will need to get a mapsApiKey for your project.
                // See: https://developers.google.com/chart/interactive/docs/basic_load_libs#load-settings
                'mapsApiKey': 'AIzaSyDD9ASiq-wmVj_e9azpW6-GbV4XL0Pwqzo'
            });
            google.charts.setOnLoadCallback(drawRegionsMap);

            function drawRegionsMap() {
                var data = google.visualization.arrayToDataTable([
                    ['Country'],
            {foreach $country_users as $country_user}
                    [{$country_user->state}],
            {/foreach}
                ]);

                var options = {
                    legend: 'none',
                    defaultColor:'#d5d0c0',
                    colorAxis: {
                        values:[0,1],
                        colors:['gray','#d5d0c0']
                        },
                    enableRegionInteractivity: true
                };

                var chart = new google.visualization.GeoChart(document.getElementById('regions_div'));

                chart.draw(data, options);
            }
        </script>
        <div class="col-lg-6 col-lg-offset-3">
            <div id="regions_div" style="width:100%;height:auto;"></div>
        </div>
</div>
<div class="row benefits-placeholder">
