<script>
    $('#more').click(function (event) {
        event.preventDefault();
        var target = "#premium-benefits";

        $('html,body').animate({
            scrollTop: $(target).offset().top - 49
        }, 800, 'easeInOutCirc');
    });

    $('#premium').click(function (event) {
        event.preventDefault();
        var target = "#premium-pricing";

        if ($(window).width() < 768)
        {
            $('html,body').animate({
                scrollTop: $(target).offset().top - 49
            }, 800, 'easeInOutCirc');
        } else
        {
            $('html,body').animate({
                scrollTop: $(target).offset().top - 139
            }, 800, 'easeInOutCirc');
        }
    });
</script>
